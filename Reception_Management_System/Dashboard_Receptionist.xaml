                <ComboBox FontSize="24" FontFamily="Helvetica">
                    <ComboBoxItem IsSelected="True" Content="2019" />
                    <ComboBoxItem Content="2020" />
                </ComboBox>

                <TextBlock Text="To: " Margin="50,0,10,0" FontSize="24" FontFamily="Helvetica" />
                <ComboBox FontSize="24" FontFamily="Helvetica">
                    <ComboBoxItem IsSelected="True" Content="01" />
                    <ComboBoxItem Content="02" />
