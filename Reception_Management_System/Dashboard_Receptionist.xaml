                </ComboBox>
                
                <TextBlock Text="To: " Margin="20,0,10,0" FontSize="24" FontFamily="Helvetica" />
                <ComboBox x:Name="CBTime2" FontSize="24" FontFamily="Helvetica">
                    <ComboBoxItem Content="08:00:00" />
                    <ComboBoxItem Content="09:00:00" />
